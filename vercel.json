{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/",
      "destination": "/"
    },
    {
      "source": "/admin",
      "destination": "/"
    },
    {
      "source": "/admin/(.+)",
      "destination": "/$1"
    },
    {
      "source": "/(.+)",
      "destination": "/$1"
    }
  ]
}
