{
  "name": "vercel-typescript-express-api",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/payload.config.ts"]
      }
    }
  ],
  "routes": [
    {
      "src": "/robots.txt",
      "status": 204
    },
    {
      "src": "/favicon.ico",
      "status": 204
    },
    {
      "src": "/(.*)",
      "dest": "/src/server.ts"
    }
  ]
}
