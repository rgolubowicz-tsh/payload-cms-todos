{
  "version": 2,
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/$1"
    },
    { "source": "/(.*)", "destination": "/api" }
  ],
  "functions": {
    "api/index.ts": {
      "includeFiles": "build/**"
    }
  },
  "outputDirectory": "build",
  "buildCommand": "yarn build"
}
